  </servlet>
  
   <servlet-mapping>
    <servlet-name>LoginSv</servlet-name>
    <url-pattern>/LoginSv</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>registrerServelt</servlet-name>
