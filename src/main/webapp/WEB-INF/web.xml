  	<servlet-name>AssuranceServlet</servlet-name>
  	<url-pattern>/newA</url-pattern>
  	<url-pattern>/insertA</url-pattern>
  	<url-pattern>/deleteA</url-pattern>
  	<url-pattern>/editA</url-pattern>
  	<url-pattern>/updateA</url-pattern>
  	<url-pattern>/listA</url-pattern>
  </servlet-mapping>
  
   <servlet>
